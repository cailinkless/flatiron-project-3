<%# Program Listing Section %>

<% if (current_user && current_user.id == @production.user.id) || @production.published == true %>

    <%= render 'listing' %>

<% else %>

    <%= render 'unknown' %>

<% end %>

<%# Edit Menu Section %>

<% if current_user && current_user.id == @production.user.id && @production.published == true %>

    <%= render 'publishedmenu' %>

<% elsif current_user && current_user.id == @production.user.id && @production.published == false %>

    <%= render 'unpublishedmenu' %>

<% end %>

<%# Explore Section %>

<%= render 'explore' %>